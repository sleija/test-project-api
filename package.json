{
  "name": "test-project-api",
  "version": "1.0.0",
  "description": "The API for Test Project",
  "main": "src/index.ts",
  "private": "true",
  "engines": {
    "node": "16.x",
    "npm": "8.x"
  },
  "scripts": {
    "build": "tsc && cp -R src/static dist/src",
    "start": "node dist/src/index.js",
    "dev": "nodemon -r tsconfig-paths/register --watch 'src/**/*.ts' --exec ts-node src/index.ts",
    "dev.win": "ts-node-dev --respawn --notify false --prefer-ts --ignore-watch node_modules -- src/index.ts",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx --max-warnings=10",
    "debug": "nodemon --watch 'src/**/*.ts' --exec node --inspect -r ts-node/register src/index.ts",
    "lint:fix": "npm run lint -- --fix",
    "test": "jest --watchAll",
    "test:coverage": "jest --coverage"
  },
  "dependencies": {
    "@types/cron": "~1.7.2",
    "@types/dom-parser": "~0.1.1",
    "@types/multer": "~1.4.7",
    "@types/supertest": "~2.0.10",
    "@usefultools/utils": "~6.3.1",
    "atob": "~2.1.2",
    "axios": "~0.26.1",
    "btoa": "~1.2.1",
    "cors": "~2.8.5",
    "cron": "~1.8.2",
    "crypto": "~1.0.1",
    "date-fns": "~2.28.0",
    "dom-parser": "~0.1.6",
    "dotenv": "~8.6.0",
    "express": "~4.17.1",
    "express-async-errors": "~3.1.1",
    "express-basic-auth": "~1.2.0",
    "express-jwt": "~6.0.0",
    "express-jwt-authz": "~2.4.1",
    "got": "~11.8.2",
    "helmet": "~4.6.0",
    "html-pdf": "~3.0.1",
    "http-status": "~1.5.0",
    "jsonwebtoken": "~8.5.1",
    "jwks-rsa": "~1.12.1",
    "jwt-decode": "~3.1.2",
    "lodash": "~4.17.21",
    "mixpanel": "~0.14.0",
    "multer": "~1.4.4",
    "newrelic": "~8.7.1",
    "nodemailer": "~6.7.3",
    "openapi-types": "~10.0.0",
    "pg": "~8.7.3",
    "pg-hstore": "~2.3.3",
    "react": "~16.13.1",
    "react-dom": "~16.13.1",
    "read-excel-file": "~5.2.28",
    "readable-stream-clone": "~0.0.7",
    "request": "~2.88.2",
    "sequelize": "~6.16.2",
    "stream-chat": "~3.13.1",
    "streamifier": "~0.1.1",
    "swagger-jsdoc": "~6.1.0",
    "swagger-ui-express": "~4.3.0",
    "uuid": "~8.3.2",
    "write-excel-file": "~1.3.16",
    "ws": "~7.5.7"
  },
  "devDependencies": {
    "@types/btoa": "~1.2.3",
    "@types/cors": "~2.8.7",
    "@types/express": "~4.17.8",
    "@types/html-pdf": "~2.2.1",
    "@types/jest": "~26.0.13",
    "@types/lodash": "~4.14.165",
    "@types/nodemailer": "~6.4.0",
    "@types/pg": "~7.14.7",
    "@types/streamifier": "~0.1.0",
    "@types/swagger-jsdoc": "~6.0.0",
    "@types/uuid": "~8.3.0",
    "@typescript-eslint/eslint-plugin": "~4.13.0",
    "@typescript-eslint/parser": "~4.13.0",
    "casual": "~1.6.2",
    "eslint": "~7.17.0",
    "eslint-config-prettier": "~6.15.0",
    "eslint-import-resolver-typescript": "~2.5.0",
    "eslint-plugin-filenames": "~1.3.2",
    "eslint-plugin-import": "~2.22.1",
    "eslint-plugin-jest": "~24.1.3",
    "eslint-plugin-prettier": "~3.3.1",
    "husky": "~4.3.7",
    "jest": "~26.6.3",
    "lint-staged": "~10.5.3",
    "nock": "~13.0.5",
    "nodemon": "~2.0.4",
    "prettier": "~2.2.1",
    "sequelize-cli": "~6.2.0",
    "sequelize-mock-v5": "~1.2.3",
    "supertest": "~5.0.0",
    "ts-jest": "~26.4.4",
    "ts-node": "~9.1.1",
    "ts-node-dev": "~1.1.8",
    "typescript": "~4.1.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.{js,jsx,ts,tsx,json,css,scss,md}": [
      "prettier --write"
    ]
  }
}
